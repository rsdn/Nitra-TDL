    UniqueId = "7E9EDB31-B4C7-431A-AD51-EAAA52498E6A";
    TfsId = 0;
    Timeout = "00:00:10";
}

suite Suite1
{
  platform win7-x64;
  product P1;
  InitialDeploymentParameter = "A";
