{
  "version": 7,
  "$schema": "http://auto-reports.avp.ru/starter-schema/v7/prod/schema.json",
  "DefaultValues": {
    "UpdateSource": ""
  },
  "Deployments": {
    "InstallProduct": {
      "Type": "Script",
      "ReturnValue": 0,
      "ScriptArgs": {
        "BuildDir": "$(BuildDir)",
        "BuildPath": "$(BuildPath)",
        "ProductInstall": "$(ProductInstall)",
        "TestArchive": "$(TestArchive)",
        "DEPLOY_ROBOT": "$(DEPLOY_ROBOT)",
        "E_AIR_ACCESSKEY": "$(E_AIR_ACCESSKEY)",
        "E_AIR_CLI": "$(E_AIR_CLI)",
        "E_BUILD_PKG": "$(E_BUILD_PKG)",
        "E_STORAGE": "$(E_STORAGE)",
        "PYLIBS": "$(PYLIBS)",
        "TEST_ARC_PATH": "$(BuildPath)\\$(BuildDir)\\$(TestArchive)",
        "E_TESTS_DIR": "$(TestsDir)",
        "E_BUILD_PATH": "$(BuildPath)\\$(BuildDir)",
        "E_PRODUCT_VERSION": "$(Version)",
        "A": "$(Product:SecondA.Size)",
        "B": "$(Product:SecondB.Size)"
      },
      "DefaultValues": {
        "DEPLOY_ROBOT": "hello_deploy.robot",
        "E_AIR_ACCESSKEY": "{AirToken}",
        "E_AIR_CLI": "air-cli",
        "E_BUILD_PKG": "MonrepoHelloWorld",
        "E_STORAGE": "kesl.domain.com",
        "PYLIBS": "pexpect,requests,robotframework"
      },
      "ScriptPath": "$(BuildPath)\\$(BuildDir)\\$(ProductInstall)"
    },
    "InstallTestAgent": {
      "Type": "Script",
      "ReturnValue": 0,
      "ScriptArgs": {},
      "DefaultValues": {},
      "ScriptPath": "\\\\domain.com\\global\\Builds2\\Ido-Dev\\TestAgent.Python-monorep\\1.2.0.404\\drop\\Domain.Hive.TestAgent\\install_linux.sh"
    },
    "LinuxDeploy": {
      "Type": "Sequence",
      "Deployments": [
        "InstallTestAgent",
        "InstallProduct"
      ]
    }
  },
  "Platforms": {
    "cent75-x64": {}
  },
  "Products": {
    "Excel": {
      "Name": "Excel",
      "Parameters": {
        "Name": "Excel",
        "Size": 42
      },
      "BranchName": "Excel",
      "Version": "#(ProductVersion)"
    },
    "MonorepoHelloWorld": {
      "Name": "MonorepoHelloWorld",
      "Parameters": {
        "Name": "MonorepoHelloWorld"
      },
      "BranchName": "MonorepoHelloWorld",
      "Version": "#(ProductVersion)"
    },
    "Word": {
      "Name": "Word",
      "Parameters": {
        "Name": "Word",
        "Size": 100
      },
      "BranchName": "Word",
      "Version": "#(ProductVersion)"
    }
  },
  "Suites": {
    "BuildAcceptance": {
      "Parameters": {
        "Version": "#(ProductVersion)",
        "BuildPath": "#(BuildPath)",
        "BuildDir": "distrib\\linux-x86_64",
        "SecondA": "Excel",
        "SecondB": "Word",
        "ProductInstall": "install_product.sh",
        "TestArchive": "MonrepoHelloWorld-tests.tgz",
        "TestsDir": "/root/autotests"
      },
      "Platforms": [
        "cent75-x64"
      ],
      "Products": [
        "MonorepoHelloWorld"
      ],
      "TestScenarios": [
        "BuildAcceptance"
      ]
    }
  },
  "TestScenarios": {
    "BuildAcceptance": {
      "TestScenarios": [
        "FirstTestFile",
        "SecondTestFile"
      ]
    },
    "FirstTestFile": {
      "Deployments": [
        "LinuxDeploy"
      ],
      "Environments": [
        {
          "Platforms": [
            "cent75-x64"
          ],
          "Products": [
            "MonorepoHelloWorld"
          ]
        }
      ],
      "RerunCountOnTestFailure": 0,
      "RevertAgentAfter": true,
      "TestMethod": {
        "TestScriptName": "utf_robot_wrapper.sh",
        "TestScriptArgs": "\"hello_tests.robot\"",
        "Environment": {}
      },
      "Timeout": "01:00:00",
      "UniqueId": "86fa3108-085b-411f-a94b-a3c9338a58a1"
    },
    "SecondTestFile": {
      "Deployments": [
        "LinuxDeploy"
      ],
      "Environments": [
        {
          "Platforms": [
            "cent75-x64"
          ],
          "Products": [
            "MonorepoHelloWorld"
          ]
        }
      ],
      "RerunCountOnTestFailure": 0,
      "RevertAgentAfter": true,
      "TestMethod": {
        "TestScriptName": "utf_robot_wrapper.sh",
        "TestScriptArgs": "\"hello_tests_second.robot\"",
        "Environment": {}
      },
      "Timeout": "01:00:00",
      "UniqueId": "73b136a8-2032-4abb-b5dc-85283cda1d92"
    }
  },
  "TestEntities": {}
}