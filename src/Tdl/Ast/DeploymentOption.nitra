using DotNet;
using DotNet.AstUtils;

using Nitra;
using Nitra.Declarations;

namespace Tdl
{
  abstract ast DeploymentOption : BindableAst
  {
    | Success
      {
        Value : ParsedValue[int];
      }

    | ForReboot
      {
        Value : ParsedValue[int];
      }

    | Timeout
      {
        Value : CSharp.StringLiteral;
      }
  }
}
